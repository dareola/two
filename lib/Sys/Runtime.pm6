                %wFieldInfo{'tablfld'} = %wTABLFLDS<tabname>.Str ~ '-' ~ %wTABLFLDS<fldname>.Str;
                my Str $data-element = %wFieldInfo<delemnt>.Str;

                #-- Text elements
